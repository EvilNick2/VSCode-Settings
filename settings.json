{
	"files.encoding": "utf8",
	"editor.fontFamily": "'Consolas', 'monospace'",
	"window.title": "${activeEditorShort}${separator}${rootName}",
	"editor.wordWrap": "off",
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.snippetSuggestions": "top",
	"editor.fontSize": 12,
	"editor.insertSpaces": false,
	"editor.autoIndent": "advanced",
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.tokenColorCustomizations": {
		"[Monokai]": {
			"comments": "#0F0"
		}
	},
	"workbench.startupEditor": "none",
	"workbench.iconTheme": "vscode-icons",
	"security.workspace.trust.untrustedFiles": "open",
	"security.workspace.trust.startupPrompt": "never",
	"security.workspace.trust.enabled": false,
	"vsicons.dontShowNewVersionMessage": true,
	"explorer.confirmDelete": false,
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"gitlens.currentLine.enabled": false,
	"gitlens.hovers.currentLine.over": "line",
	"git.autofetch": true,
	"workbench.colorTheme": "Bearded Theme Black & Amethyst",
	"workbench.editor.empty.hint": "hidden",
	"editor.tabSize": 2,
	"editor.inlineSuggest.showToolbar": "always",
	"github.copilot.advanced": {},
	"github.copilot.editor.enableAutoCompletions": false,
	"color-highlight.markerType": "outline",
	"editor.formatOnSave": true,
	"workbench.editorAssociations": {
		"*.class": "default"
	},
	"simple.readme.settings.github": "EvilNick2",
	"simple.readme.settings.name": "EvilNick2",
	"simple.readme.settings.lang": "en",
	"github.copilot.editor.enableCodeActions": false,
	"files.exclude": {
		"**/.git": false
	},
	"license.default": "mit",
	"license.author": "EvilNick2",
	"markdown-preview-enhanced.previewTheme": "github-dark.css"
}